<!DOCTYPE	html>
<html>
<head>
    <meta name="viewport" content="width=device-width,initial-scale=1.0" charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/content/bootstrap.css")" rel="stylesheet" />

    @RenderSection("styles", false)
</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark navbar-fixed-top m-0">
        <a class="navbar-brand" href="@Url.Action("Index","Home")">
            <img src="@Url.Content("~/Content/img/blacklogo.png")" width="130" height="35" alt="">
        </a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item">
                    <a class="nav-link disabled" href="#" tabindex="-1" aria-disabled="true">Dashboard</a>
                </li>

                <li class="nav-item">
                    <a class="nav-link disabled" href="#" tabindex="-1" aria-disabled="true">CheckList</a>
                </li>

                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="#" id="navbarDropdownMenuLink" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        Ocorrências
                    </a>
                    <div class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
                        <a>@Html.RouteLink("Lista de Ocorrências", "OcorrenciaIndex", "Ocorrencia", new { @class = "dropdown-item" })</a>
                        <a>@Html.RouteLink("Adicionar Ocorrência", "OcorrenciaCreate", "Ocorrencia", new { @class = "dropdown-item" })</a>
                    </div>
                </li>

                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="#" id="navbarDropdownMenuLink" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        Servidores
                    </a>
                    <div class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
                        <a>@Html.RouteLink("Lista de Servidores", "ServidorIndex", "Servidor", new { @class = "dropdown-item" })</a>
                        <a>@Html.RouteLink("Adicionar Servidor", "ServidorCreate", "Servidor", new { @class = "dropdown-item" })</a>
                    </div>
                </li>

                <li class="nav-item active">
                     <span class="sr-only">(current)</span>
                </li>

                <li class="nav-item active">
                     <span class="sr-only">(current)</span>
                </li>

                <li class="nav-item">
                    <a class="nav-link disabled" href="#" tabindex="-1" aria-disabled="true">Rotinas</a>
                </li>

                <li class="nav-item">
                    <a class="nav-link disabled" href="#" tabindex="-1" aria-disabled="true">Manuais</a>
                </li>

                <li class="nav-item">
                    <a class="nav-link disabled" href="#" tabindex="-1" aria-disabled="true">Contatos</a>
                </li>



            </ul>
        </div>
    </nav>
    <div class="col-xs-9	col-md-9">
        <br />
        @RenderBody()
    </div>
    <script src="@Url.Content("~/scripts/jquery-1.10.2.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery-3.0.0.slim.min.js")"></script>
    <script src="@Url.Content("~/scripts/bootstrap.js")"></script>
    <script src="@Url.Content("~/scripts/popper.min.js")"></script>
    @RenderSection("ScriptPage",
false);
</body>
</html>