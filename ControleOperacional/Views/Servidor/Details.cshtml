@model Modelo.Entidades.Servidor

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Visualizando detalhes de um Servidor";
}
<div class="panel panel-primary">
    <div class="panel-heading">
        Visualizando detalhes de um Servidor
    </div>
    <div class="panel-body">
        @Html.Partial("~/Views/Servidor/_PartialDetaislsContentPanel.cshtml", Model)
    </div>
    <div class="panel-footer panel-info">
        @Html.ActionLink("Alterar", "Edit", new { id = Model.ServidorId }, new { @class = "btn	btn-info" })
        @Html.ActionLink("Retornar para Listagem", "Index", null, new { @class = "btn btn-info" })
    </div>
</div>